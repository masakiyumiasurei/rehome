@model PopupSearchSiireModel
@using rehome.Models.DB;
@using rehome.Enums;


<div class="modal fade" id="ModalWindow" tabindex="-1" role="dialog" aria-labelledby="ModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content">

            <div class="modal-header">
                <h3>仕入先検索</h3>
                <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
            </div>
            <div class="modal-body">
                <div class="row g-1">                   
                    <div class="col-4"><input type="text" id="PopupSiireName" size="10" class="form-control form-control-sm" placeholder='仕入先名'></div>
                    <div class="col-2">
                        @Html.DropDownList("業種", new SelectList(Enum.GetValues(typeof(業種))),"業種を選択", new { @class = "form-control form-control-sm", @placeholder = "業種", @id="PopupSiireGyoushu" })
                       
                        @*@Html.DropDownListFor(model => model.SearchOffices, new SelectList(販売店.Rankitems(), "Value", "Text"),
                        "ランク",new { @class = "form-control",@placeholder="ランク",@id = "PopupOfficerank" })*@
                    </div>
                    <div class="col-auto"><button type="button" class="btn-sm btn-primary" id="button" onclick="PopupSearch_Siire()">検索</button></div>
                </div>

                <div class="col-12 col-sm-12">
                    <div style="text-align: center"><img src="~/icon/Ghost.gif" width="80" height="80" alt="Now Loading..." id="loading" style="display: none" /></div>
                    <div id="PopupResultView">
                        <br>
                        <h6>  検索してください</h6>
                        
                    </div>
                </div>
            </div>

            <input data-val="true" id="CallerName" type="hidden">
            <input data-val="true" id="PopupCallURL" type="hidden">
        </div>
    </div>
</div>
