@model PopupSearchSyouhinModel

    <table  class="table table-striped table-hover table-sm ">
        <thead>
            <tr>
            <th class="column5">選択</th>
            <th class="column5">商品ID</th>
                <th>商品名</th>               
                
            </tr>
        </thead>
        <tbody>
            @if (Model.SearchSyouhins != null)
            {
                @foreach (var item in Model.SearchSyouhins)
                {
                    <tr>
                        <td >
                        <button type="button" class="custom-btn btn-size-min btn-1" id="PopupSelect" data-bs-dismiss="modal"><i class="fa fa-check" aria-hidden="true"></i></button>
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.商品ID)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.商品名)
                        </td>
                       
                        
                    </tr>
                }
            }
        </tbody>
    </table>


@section Scripts{

}