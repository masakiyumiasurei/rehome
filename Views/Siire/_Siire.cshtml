@using rehome.Enums;
@using rehome.Models.DB;
@model SiireDetailModel
<div class="row">

    @Html.HiddenFor(m => m.Siire.仕入先ID)

            <div class="row">
             <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
              <div class="tile-box">
                <label for="name" class="col-xs-1 tile-label2">仕入先名</label>
                    @Html.TextBoxFor(m => m.Siire.仕入先名, new { @class = "form-control" })
              </div>
              </div></div>


               <div class="row">
               <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
              <div class="tile-box">
                <label for="name" class="col-xs-1 tile-label2">郵便番号</label>
                    @Html.TextBoxFor(m => m.Siire.郵便番号, 
                        new { @class = "form-control" , onKeyUp="AjaxZip3.zip2addr(this,'','Siire.住所','Siire.住所');" ,@id="zipcode" })
                </div></div></div>

            <div class="row">
             <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
              <div class="tile-box">
                <label for="name" class="col-xs-1 tile-label2">住所</label>
                    @Html.TextBoxFor(m => m.Siire.住所, new { @class = "form-control" ,@id="address"})
            </div></div>
            </div>


             <div class="row">
             <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
              <div class="tile-box">
                <label for="name" class="col-xs-1 tile-label2">肩書</label>
                    @Html.TextBoxFor(m => m.Siire.肩書, new { @class = "form-control" })

                </div>
               </div></div>

                <div class="row">
            <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
              <div class="tile-box">
                <label for="name" class="col-xs-1 tile-label2">TEL</label>
                    @Html.TextBoxFor(m => m.Siire.TEL, new { @class = "form-control", @type="tel"})
            </div></div>
            </div>


             <div class="row">
            <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
              <div class="tile-box">
                <label for="name" class="col-xs-1 tile-label2">FAX</label>
                    @Html.TextBoxFor(m => m.Siire.FAX, new { @class = "form-control", @type="tel"})
            </div></div>
            </div>

               </div>





           

  




              
    <div>@Html.HiddenFor(a => a.Mode)</div>
     @Html.HiddenFor(a => a.BackUrl)
</div>